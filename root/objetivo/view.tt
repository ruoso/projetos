
<table name="view">

<tr>
<td class="view_label"><b>Projeto:</b></td>
<td>
    
    [%  item.projeto %]
    
</td>
</tr>

<tr>
<td class="view_label"><b>Tipo Objetivo:</b></td>
<td>
    
    [%  item.tipo_objetivo %]
    
</td>
</tr>

<tr>
<td class="view_label"><b>Metas:</b></td>
<td>
    
    [% FOR val = item.metas; val; ', '; END %]
    
</td>
</tr>

<tr>
<td class="view_label"><b>Data Inicio:</b></td>
<td>
    
    [%  item.data_inicio %]
    
</td>
</tr>

<tr>
<td class="view_label"><b>Data Fim:</b></td>
<td>
    
    [%  item.data_fim %]
    
</td>
</tr>

<tr>
<td class="view_label"><b>Descricao:</b></td>
<td>
    
    [%  item.descricao %]
    
</td>
</tr>

</table>
<hr />
<a href="[% c.uri_for( 'edit', item.objetivo_id,  ) %]">Edit</a>
<hr />
<a href="[% c.uri_for( 'list' ) %]">List</a>


