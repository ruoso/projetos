
<table>
<tr>

<th> [% order_by_column_link('descricao', 'Descricao') %] </th>

<th> [% order_by_column_link('data_inicio', 'Data Inicio') %] </th>


<th> [% order_by_column_link('data_fim', 'Data Fim') %] </th>


 
</tr>
[% WHILE (row = result.next) %]
    <tr>
    
    <td>
    
    [%  row.descricao %]
    
    </td>
    
    <td>
    
    [%  row.data_inicio %]
    
    </td>
    
    <td>
    
    [%  row.data_fim %]
    
    </td>
    
     
    [% SET id = row.$pri %]
    <td><a href="[% c.uri_for( 'view', row.papel_id,  ) %]">View</a></td>
    <td><a href="[% c.uri_for( 'edit', row.papel_id,  ) %]">Edit</a></td>
    <td><a href="[% c.uri_for( 'destroy', row.papel_id,  ) %]">Delete</a></td>
    </tr>
[% END %]
</table>
[% PROCESS pager.tt %]
